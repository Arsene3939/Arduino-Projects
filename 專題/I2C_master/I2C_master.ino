const byte lcd_address=0x09,SD178_address=0x20;
byte tongue[13] = {
  0x8A,0x00,                  // set SD178BM output pins MO[2:0] = 0b000
  0x8A,0x04,                  // set SD178BM output pins MO[2:0] = 0b100
  0x86,0xD0,
  0x88,0x27,0x0F,0x00,0x06,   // 播放 microSD 9998.WAV 2遍
  //0XA5,0XCB,0XB7,0X66,0XA6,0XE8,0XAB,0XF5,0XA4,0X4B,0XB9,0XC7,0XAA,0XBA,0XA6,0XBA,
  0x8A,0x00                   // set SD178BM output pins MO[2:0] = 0b000
};
void setup() {
  // Start the I2C Bus as Master
  Wire.begin();
  delay(10); // waits 10ms
}
void loop() {
  Wire.beginTransmission(SD178_address); // transmit to slave device
  Wire.write(tongue,13);
  Wire.endTransmission();     // stop transmitting
  Wire.beginTransmission(lcd_address); // transmit to device #9
  Wire.write(0x55);              // sends x
  Wire.endTransmission();     // stop transmitting
  delay(100);
  for(int i=0;i<1024;i++){
    Wire.beginTransmission(lcd_address); // transmit to device #9
    Wire.write(kimetsu[i]);              // sends x
    Wire.endTransmission();    // stop transmitting
    delayMicroseconds(2000);
  }
  delay(3000);
  Wire.beginTransmission(lcd_address); // transmit to device #9
  Wire.write(0x55);              // sends x
  Wire.endTransmission();    // stop transmitting
  delay(100);
  for(int i=0;i<1024;i++){
    Wire.beginTransmission(lcd_address); // transmit to device #9
    Wire.write(kaguyachan[i]);              // sends x
    Wire.endTransmission();    // stop transmitting
    delayMicroseconds(2000);
  }
}
